{
  "id": "c",
  "name": "C",
  "caseSensitive": true,
  "extensions": [".c", ".h"],
  "keywords": [
    "auto",
    "break",
    "case",
    "char",
    "const",
    "continue",
    "default",
    "do",
    "double",
    "else",
    "enum",
    "extern",
    "float",
    "for",
    "goto",
    "if",
    "inline",
    "int",
    "long",
    "register",
    "restrict",
    "return",
    "short",
    "signed",
    "sizeof",
    "static",
    "struct",
    "switch",
    "typedef",
    "union",
    "unsigned",
    "void",
    "volatile",
    "while",
    "_Bool",
    "_Complex",
    "_Imaginary"
  ],
  "multiWordKeywords": [
    { "pattern": ["unsigned", "long", "int"], "role": "type-alias" },
    { "pattern": ["signed", "long", "int"], "role": "type-alias" }
  ],
  "operators": [
    ">>=",
    "<<=",
    "...",
    "->",
    "++",
    "--",
    "==",
    "!=",
    "<=",
    ">=",
    "&&",
    "||",
    "+=",
    "-=",
    "*=",
    "/=",
    "%=",
    "&=",
    "|=",
    "^=",
    "<<",
    ">>",
    "+",
    "-",
    "*",
    "/",
    "%",
    "&",
    "|",
    "^",
    "~",
    "!",
    "<",
    ">",
    "=",
    "?:",
    "?:"
  ],
  "punctuation": ["{", "}", "(", ")", "[", "]", ";", ",", "."],
  "comments": {
    "line": ["//"],
    "block": [{ "start": "/*", "end": "*/" }]
  },
  "stringDelimiters": ["\"", "'"],
  "numberRules": {
    "allowHex": true,
    "allowOctal": true,
    "allowBinary": false,
    "allowFloat": true,
    "exponent": true,
    "suffixes": ["u", "U", "l", "L", "ul", "UL", "llu", "LLU"]
  },
  "declarationPatterns": [
    { "pattern": ["type", "identifier", "("], "role": "function-declaration" }
  ],
  "tokenTypeMap": {
    "keyword": "keyword",
    "identifier": "identifier",
    "string": "string",
    "number": "number",
    "operator": "operator",
    "punctuation": "punctuation",
    "comment": "comment"
  }
}
